programa {
  funcao inicio() {
    //Declaração de Váriaveis
    real C_celsius, F_fahrenheit

    //Entrada de Dados
    escreva ("Informe os graus Celsius: ")
    leia(C_celsius)

    //Processamento
    F_fahrenheit = (9 * C_celsius + 160) / 5

    //Saida de Dados
    escreva ("A conversão de Celsius para fahrenheit é: ", F_fahrenheit)
     
  }
}
